function Redondea(number,X)  // redondea un n√∫mero con los decimales  
 {  
	X=(!X?2:X)
	return Math.round(number*Math.pow(10,X)) / Math.pow(10,X)
 }
 function ValidaNumero(event,Entero) //si no pasa nada permite decimales
   {  
    var event1;
	
	if (!event) event = window.event;
	if (event.target) event1 = event.target || event.srcElement;
	else if (event.srcElement) event1 = event.srcElement;
	
	sValue = event1.value;
	
	Entero=(!Entero?2:Entero)
	if (Entero == 4)	  	
		event1.value = Redondea(sValue,4)			
	if (Entero == 3)	  	
		event1.value = Redondea(sValue,3)			
	if (Entero == 2)	  	
		event1.value = Redondea(sValue,2)		
	if (Entero == 1)	  
		event1.value = Math.round(sValue*Math.pow(10,0))
  }

