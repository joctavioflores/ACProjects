function FechaHoy()   // regresa la fecha de d√≠a
{
  var d, s = "";
  d = new Date();
  s += d.getDate() + "/";
  s += (d.getMonth() + 1) + "/";  
  s += d.getFullYear();
  return(s);
}         

function FechaAnt()   // regresa la fecha 8 dias antes de la actual
{
  var d, s = "";
  d = new Date();
  s += (d.getDate()-7)+ "/";
  s += (d.getMonth() + 1) + "/";  
  s += d.getFullYear();
  return(s);
} 

function Hora()     //regresa la hora en el momento
{
  var d, s = "";
  d = new Date();
  s += d.getHours() + ":";
  s += d.getMinutes()
  return(s);
}

